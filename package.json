{
  "name": "botbuilder-redis",
  "version": "0.0.1",
  "description": "Redis adapter for Microsoft BotBuilder.",
  "author": "Yuichi Watanabe",
  "license": "MIT",
  "main": "dist/botbuilder-redis.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/vvatanabe/botbuilder-redis.git"
  },
  "babel": {
    "presets": [
      "es2015",
      "babel-preset-power-assert"
    ],
    "plugins": [
      "add-module-exports",
      "es6-promise",
      "transform-class-properties",
      "syntax-flow",
      "transform-flow-strip-types"
    ]
  },
  "scripts": {
    "test": "mocha --require test/utils/dom.js --compilers js:babel-register test/**/test-*.js",
    "test-cov": "babel-node node_modules/.bin/babel-istanbul cover node_modules/.bin/_mocha --report lcovonly -- ./test",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "npm run build-lib && npm run build-dist && npm run build-minify",
    "build-dist": "browserify --debug --standalone BotbuilderRedis lib/main.js -o dist/botbuilder-redis.js",
    "build-minify": "uglifyjs dist/botbuilder-redis.js -o dist/botbuilder-redis.min.js",
    "build-lib": "babel src --out-dir lib",
    "flow": "$(npm bin)/flow",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-istanbul": "^0.8.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-es6-promise": "^1.0.0",
    "babel-plugin-syntax-flow": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "coveralls": "^2.11.9",
    "es6-promise": "^3.2.1",
    "flow-bin": "^0.27.0",
    "flowtype": "^1.0.0",
    "mocha": "^2.5.3",
    "power-assert": "^1.4.1",
    "uglify-js": "^2.6.2"
  },
  "keywords": [
    "bot",
    "botbuilder",
    "microsoft",
    "redis"
  ]
}
